      $("#off-light").click(function () {
        $(this).removeClass("active");
        $(".anime_player").removeClass("active");
      });
      $("#toggle-resize").click(function () {
        $(".page-watch").toggleClass("full-size");
      });
      $(".item.item-check").click(function () {
        $(this).toggleClass("active");
      });
